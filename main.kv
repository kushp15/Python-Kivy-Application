WindowManager:

    MainWindow:
    First:

    Add_Update:
    ViewData:
    Report:

# Main Window

<MainWindow>:
    name: "main"

    MDFloatLayout:
        md_bg_color: "white"
        radius: [0, 0, 0, 0]

        Image:
            source: 'download.jpeg'
            size_hint: 0.675, 0.5
            pos_hint: {"center_x": .5, "center_y": .75}
            allow_stretch: True
            md_bg_color: 0,0,0,1

        MDRectangleFlatButton:
            markup : True
            text: "[b]First Screen[/b]"
            theme_text_color: "Custom"
            text_color: "blue"
            line_color: "blue"
            font_size:  ( self.width + (2 *  self.height))/ 20
            size_hint_x: 0.455
            size_hint_y: 0.1
            pos_hint: {"center_x": .5, "center_y": .4}
            on_release: app.root.current = "First"
            on_release: root.manager.transition.direction = "left"


# Inside the Building-901 Second Screen:
# Second Screen Provides the information with the Add / Update / View

<First>:
    name: "First"
    MDFloatLayout:

        radius: [0, 0, 0, 0]
        MDLabel:
            markup: True
            text: "[b] Heading [/b][/color]"
            text_color: "lightblue"
            #size_hint: 0.455, 0.08
            font_size: 24
            halign: "center"
            pos_hint: {"center_x": .5, "center_y": .85}

        MDRectangleFlatButton:
            text: "Adding Data"
            size_hint: 0.455, 0.08
            theme_text_color: "Custom"
            text_color: "blue"
            line_color: "black"
            font_size:  ( self.width + (2 *  self.height))/ 20
            pos_hint: {"center_x": .5, "center_y": .7}
            on_release:
                app.root.current = "Add_Update"
                root.manager.transition.direction = "right"

        MDRectangleFlatButton:
            text: "View Data"
            size_hint: 0.455, 0.08
            theme_text_color: "Custom"
            text_color: "blue"
            line_color: "black"
            font_size:  ( self.width + (2 *  self.height))/ 20
            pos_hint: {"center_x": .5, "center_y": .6}

            on_release:
                app.root.current = "ViewData"
                root.manager.transition.direction = "right"


        MDRectangleFlatButton:
            text: "Report"
            theme_text_color: "Custom"
            text_color: "blue"
            line_color: "black"
            font_size:  ( self.width + (2 *  self.height))/ 20
            size_hint: 0.455, 0.08
            pos_hint: {"center_x": .5, "center_y": .5}

            on_release:
                app.root.current = "Report"
                root.manager.transition.direction = "right"


        MDRectangleFlatButton:
            text: "Go Back"
            size_hint: 0.455, 0.08
            text_color: "blue"
            line_color: "black"
            font_size:  (self.width + (2 *  self.height))/ 20
            size_hint: 0.455, 0.08
            pos_hint: {"center_x": .5, "center_y": .2}

            on_release:
                app.root.current = "main"
                root.manager.transition.direction = "right"


<Report>:
    name: "Report"

    MDBoxLayout:
        size_hint_y: .09
        pos_hint: {"top": 1, "x": 0}
        md_bg_color: app.theme_cls.primary_color
        MDLabel:
            halign: "left"
            valign: "top"
            text : "Site"
            size_hint_x: .4
            text_color: 0, 0, 1, 1
        MDLabel:
            halign: "left"
            valign: "top"
            text : "No."
            size_hint_x: .6
            text_color: 0, 0, 1, 1
        MDLabel:
            halign: "left"
            valign: "top"
            text : "Col1"
            size_hint_x: 1
            text_color: 0, 0, 1, 1
        MDLabel:
            halign: "left"
            valign: "top"
            text : "Date1"
            size_hint_x: 1
            text_color: 0, 0, 1, 1
        MDLabel:
            halign: "left"
            valign: "top"
            text : "Date2"
            size_hint_x: 1
            text_color: 0, 0, 1, 1

    ScrollView:
        size: self.size
        size_hint_y: 0.9

        MDGridLayout:
            size_hint_y: 0.9
            height: self.minimum_height
            width: self.minimum_width
            cols: 5

            MDLabel:
                id: site
                size_hint_x: .4
                halign: "left"
                valign: "top"
                text_color: 0, 0, 1, 1

            MDLabel:
                id: No
                size_hint_x: .5
                halign: "left"
                valign: "top"
                text_color: 0, 0, 1, 1

            MDLabel:
                id: Col
                halign: "left"
                size_hint_x: 1
                valign: "top"
                text_color: 0, 0, 1, 1

            MDLabel:
                id: date
                halign: "left"
                size_hint_x: 1
                valign: "top"
                text_color: 0, 0, 1, 1

            MDLabel:
                id: date2
                halign: "left"
                size_hint_x: 1
                valign: "top"
                text_color: 0, 0, 1, 1


    Button:
        size_hint: 0.356, 0.06
        text: "Show Records"
        pos_hint: {"top": 0.05, "x": 0.0}
        on_release:
            root.show_record()

    TextInput:
        id : number
        text: ""
        size_hint: 0.355, 0.06
        pos_hint: {"top": 0.05, "x": 0.355}
        multiline: False

    Button:
        size_hint: 0.355, 0.06
        text: "Go Back"
        pos_hint: {"top": 0.05, "x": 0.645}
        on_release:
            app.root.current = "Building 901"
            root.manager.transition.direction = "left"




